@model IEnumerable<EPSN.Trainee_courses>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Registration_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cours.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trainee.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Registration_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cours.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trainee.Name)
        </td>

    </tr>
}

</table>
